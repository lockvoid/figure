{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noExternalResolve": false,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es6"
  }
}
